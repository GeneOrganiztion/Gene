  <select id="selectOderAndProductByOrderId" resultMap="orderAndProduct" parameterType="java.lang.Integer">
  	 select 
		 mp.`map_order_product_id`,
		 mp.`ord_id`,
		 mp.`pro_classify_id`,
		 mp.`pro_price`,
		 mp.`pro_count`,
		 mp.`report_is_upload`,
		 pt.`product_id`,
		 pt.`pro_name`,
		 pt.`pro_head`,
		 pt.`product_price`,
		 pt.`pro_sum`,
		 pt.`pro_rateprice`,
		 pt.`pro_online`,
		 pt.`pro_image`,
		 pt.`pro_detail`,
		 pt.`pro_area`,
		 pt.`classify_id`
	from map_order_product mp ,product pt where 
	mp.`pro_id`= pt.`product_id` 
	and mp.`isdelete` = 0 
	and pt.`isdelete` = 0 
	and mp.`ord_id`= #{orderId}
  </select>
  
  
